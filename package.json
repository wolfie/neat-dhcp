{
  "name": "neat-dhcpd",
  "version": "0.0.0",
  "license": "ISC",
  "engines": {
    "node": "20.10.0",
    "pnpm": ">=8.14"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:db": "pnpm --filter \"@neat-dhcpd/db\" dev",
    "dev:dhcpd": "pnpm --filter \"@neat-dhcpd/dhcpd\" dev",
    "dev:web-ui": "pnpm --filter \"@neat-dhcpd/web-ui\" dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "concurrently": "^8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
